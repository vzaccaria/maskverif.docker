
(*CMS scheme for t=3/s=4, with 16 randoms*)

proc iet_and_3_opt:

	inputs: a[0:3], b[0:3]
	outputs: c[0:3]
	randoms: r[0:15];

	(*randoms*)
	q_0_4 = ![r[0] + r[4]];
	q_1_5 = ![r[1] + r[5]];
	q_2_6 = ![r[2] + r[6]];
	q_3_7 = ![r[3] + r[7]];

	q_4_8 = ![r[4] + r[8]];
	q_5_9 = ![r[5] + r[9]];
	q_6_10 = ![r[6] + r[10]];
	q_7_11 = ![r[7] + r[11]];

	q_8_12 = ![r[8] + r[12]];
	q_9_13 = ![r[9] + r[13]];
	q_10_14 = ![r[10] + r[14]];
	q_11_15 = ![r[11] + r[15]];

	q_12_0 = ![r[12] + r[0]];
	q_13_1 = ![r[13] + r[1]];
	q_14_2 = ![r[14] + r[2]];
	q_15_3 = ![r[15] + r[3]];


	(*first cone, no-linear layer*)
	a0b0 := a[0] * b[0];
	a0b0 = ![a0b0 + q_0_4];
	a0b1 := a[0] * b[1];
	a0b1 = ![a0b1 + q_1_5];
	a0b2 := a[0] * b[2];
	a0b2 = ![a0b2 + q_2_6];
	a0b3 := a[0] * b[3];
	a0b3 = ![a0b3 + q_3_7];

	(*second cone, no-linear layer*)
	a1b0 := a[1] * b[0];
	a1b0 = ![a1b0 + q_4_8];
	a1b1 := a[1] * b[1];
	a1b1 = ![a1b1 + q_5_9];
	a1b2 := a[1] * b[2];
	a1b2 = ![a1b2 + q_6_10];
	a1b3 := a[1] * b[3];
	a1b3 = ![a1b3 + q_7_11];

	(*third cone, no-linear layer*)
	a2b0 := a[2] * b[0];
	a2b0 = ![a2b0 + q_8_12];
	a2b1 := a[2] * b[1];
	a2b1 = ![a2b1 + q_9_13];
	a2b2 := a[2] * b[2];
	a2b2 = ![a2b2 + q_10_14];
	a2b3 := a[2] * b[3];
	a2b3 = ![a2b3 + q_11_15];

	(*fourth cone, no-linear layer*)
	a3b0 := a[3] * b[0];
	a3b0 = ![a3b0 + q_12_0];
	a3b1 := a[3] * b[1];
	a3b1 = ![a3b1 + q_13_1];
	a3b2 := a[3] * b[2];
	a3b2 = ![a3b2 + q_14_2];
	a3b3 := a[3] * b[3];
	a3b3 = ![a3b3 + q_15_3];

	(*first cone, compression*)
	c[0] := a0b0 + a0b1;
	c[0] := c[0] + a0b2;
	c[0] := c[0] + a0b3;

	(*second cone, compression*)
	c[1] := a1b0 + a1b1;
	c[1] := c[1] + a1b2;
	c[1] := c[1] + a1b3;

	(*third cone, compression*)
	c[2] := a2b0 + a2b1;
	c[2] := c[2] + a2b2;
	c[2] := c[2] + a2b3;

	(*fourth cone, compression*)
	c[3] := a3b0 + a3b1;
	c[3] := c[3] + a3b2;
	c[3] := c[3] + a3b3;

end

verbose 1
SNI iet_and_3_opt
