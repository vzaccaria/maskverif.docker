
(*Scheme for t=2/s=3, optimization with only 6 randoms*)

proc iet_and_2_opt:

	inputs: a[0:2], b[0:2]
	outputs: c[0:2]
	randoms: q[0:5];

	(*randoms*)
	reg_r0 = ![q[0] + q[3]];
	reg_r1 = ![q[1] + q[4]];
	reg_r2 = ![q[2] + q[5]];
	
	(*first cone, no-linear layer*)
	a0b0 := a[0] * b[0]; 
	a0b0 = ![a0b0 + q[0]];
	a0b1 := a[0] * b[1];
	a0b1 = ![a0b1 + q[1]];
	a0b2 := a[0] * b[2];
	a0b2 = ![a0b2 + q[2]];
	
	(*second cone, no-linear layer*)
	a1b0 := a[1] * b[0]; 
	a1b0 = ![a1b0 + reg_r0];
	a1b1 := a[1] * b[1];
	a1b1 = ![a1b1 + reg_r1];
	a1b2 := a[1] * b[2];
	a1b2 = ![a1b2 + reg_r2];
	
	(*third cone, no-linear layer*)
	a2b0 := a[2] * b[0]; 
	a2b0 = ![a2b0 + q[3]];
	a2b1 := a[2] * b[1];
	a2b1 = ![a2b1 + q[4]];
	a2b2 := a[2] * b[2];
	a2b2 = ![a2b2 + q[5]];
	
	(*first cone, compression*)
	c[0] := a0b0 + a0b1;
	c[0] := c[0] + a0b2;
	
	(*second cone, compression*)
	c[1] := a1b0 + a1b1;
	c[1] := c[1] + a1b2;
	
	(*third cone, compression*)
	c[2] := a2b0 + a2b1;
	c[2] := c[2] + a2b2;

end

verbose 1
SNI iet_and_2_opt
