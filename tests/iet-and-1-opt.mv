
(*Scheme for t=1/s=2, optimization with only 2 randoms*)

proc iet_and_1_opt:

	inputs: a[0:1], b[0:1]
	outputs: c[0:1]
	randoms: q[0:1];

	a0b0 := a[0] * b[0]; 
	a0b1 := a[0] * b[1];
	a1b0 := a[1] * b[0];
	a1b1 := a[1] * b[1];
	
	a0b0 = ![a0b0 + q[0]];
	a0b1 = ![a0b1 + q[1]];
	a1b0 = ![a1b0 + q[0]];
	a1b1 = ![a1b1 + q[1]];
	
	c[0] = a0b0 + a0b1;
	c[1] = a1b0 + a1b1;
	

end

verbose 1
SNI iet_and_1_opt
